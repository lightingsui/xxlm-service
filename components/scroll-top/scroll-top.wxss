.tui-scroll-top {
		width: 76rpx;
		height: 76rpx;
		background: rgba(216, 216, 216, 0.6);
		border: 1rpx solid #BCBCBC;
		display: flex;
		align-items: center;
		justify-content: center;
		border-radius: 50%;
		position: fixed;
		z-index: 999999;
		-webkit-transform: translateZ(0);
		transform: translateZ(0); 
		-webkit-overflow-scroll: touch;
	}

	.tui-scroll-top-img {
		width: 48rpx;
		height: 48rpx;
		display: block;
	}