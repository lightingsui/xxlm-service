<cu-custom bgColor="bg-gradual-pink" isBack="{{true}}">
  <view slot="backText">返回</view>
  <view slot="content">团队成员</view>
</cu-custom>

<scroll-view scroll-y class="indexes" scroll-into-view="indexes-{{listCurID}}" style="height:calc(100vh - {{CustomBar}}px)" scroll-with-animation="true" enable-back-to-top="true">
  <block wx:for="{{cityList}}" wx:key>
    <view class="padding">{{item.title}}</view>
    <view class="cu-list menu-avatar no-padding"  bindtap="member">
      <view class="cu-item solid-bottom" wx:for="{{item.lists}}" wx:key="{{index}}">
        <view class="cu-avatar round lg" style="background-image:url(https://ossweb-img.qq.com/images/lol/web201310/skin/big10001.jpg);">
          <view class="cu-tag badge cuIcon-female bg-pink"></view>
        </view>
        <view class="content">
          <text class="text-abc">{{item.name}}</text>
        </view>
      </view>
    </view>
  </block>
</scroll-view>
<view class="indexBar" style="height:calc(100vh - {{CustomBar}}px - 50px)">
  <view class="indexBar-box" bindtouchstart="tStart" bindtouchend="tEnd" catchtouchmove="tMove">
    <view class="indexBar-item" wx:for="{{cityList}}" wx:key id="{{index}}" bindtouchstart="getCur" bindtouchend="setCur">{{item.title}}</view>
  </view>
</view>
<!--选择显示-->
<view hidden="{{hidden}}" class="indexToast">
  {{listCur}}
</view>